<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_image"
    tools:context=".LihatDataDiri">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

    <EditText
        android:id="@+id/lihat_jenkel"
        android:layout_width="341dp"
        android:layout_height="57dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edit_text"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="Perempuan"
        android:inputType="textPersonName"
        android:paddingLeft="70px"
        android:paddingEnd="70px"
        android:textColorHint="#615151"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/lihat_tgllahir"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/lihat_tgllahir"
        android:layout_width="340dp"
        android:layout_height="55dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edit_text"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="  12/08/2001"
        android:inputType="phone"
        android:paddingStart="70px"
        android:paddingEnd="70px"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/lihat_nohp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/poppins_light"
        android:text="Nama Pengguna"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/btn_editdatadiri"
        app:layout_constraintTop_toBottomOf="@+id/btn_editdatadiri" />

    <EditText
        android:id="@+id/lihat_nohp"
        android:layout_width="340dp"
        android:layout_height="55dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edit_text"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="  082381911111"
        android:inputType="phone"
        android:paddingStart="70px"
        android:paddingEnd="70px"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/lihat_nama"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/poppins_light"
        android:text="Tanggal Lahir"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/lihat_nohp"
        app:layout_constraintTop_toBottomOf="@+id/lihat_nohp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/poppins_light"
        android:text="Nomor HP"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/lihat_nama"
        app:layout_constraintTop_toBottomOf="@+id/lihat_nama" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/poppins_light"
        android:text="Jenis Kelamin"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/lihat_tgllahir"
        app:layout_constraintTop_toBottomOf="@+id/lihat_tgllahir" />

    <EditText
        android:id="@+id/lihat_nama"
        android:layout_width="341dp"
        android:layout_height="57dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edit_text"
        android:editable="false"
        android:ems="10"
        android:fontFamily="@font/poppins_light"
        android:hint="ulftm12"
        android:inputType="textPersonName"
        android:paddingLeft="70px"
        android:paddingEnd="70px"
        android:textColorHint="#615151"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_editdatadiri"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Lihat Data Diri"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="158dp"
        android:layout_height="155dp"
        android:layout_marginTop="60dp"
        app:cardCornerRadius="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="159dp"
            android:layout_height="157dp"
            tools:srcCompat="@tools:sample/avatars" />

    </androidx.cardview.widget.CardView>

    <Button
        android:id="@+id/btn_editdatadiri"
        android:layout_width="338dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#5EC401"
        android:drawableRight="@drawable/next"
        android:fontFamily="@font/poppins_light"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:text="E D I T    D A T A    D I R I"
        android:textAllCaps="true"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView" />

</androidx.constraintlayout.widget.ConstraintLayout>